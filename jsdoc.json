{
  "source": {
    "include": [
      "."
    ],
    "exclude": [
      "node_modules"
    ],
    "includePattern": ".+\\.mjs(doc|x)?$"
  },
  "opts": {
    "destination": "doc",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "systemName": "FutuQuant-Nodejs",
    "navType": "vertical",
    "theme": "cerulean",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "outputSourceFiles": true
  },
  "plugins": [
    "plugins/markdown"
  ]
}