{
  "name": "futuquant",
  "version": "0.2.0",
  "description": "FutuQuant-Nodejs",
  "main": "src/futuquant.js",
  "scripts": {
    "test": "mocha -r esm -t 600000 -b -w test/*",
    "doc": "rm -rf doc && jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template",
